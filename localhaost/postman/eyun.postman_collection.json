							"raw": "{\r\n\"wcId\":\"xxx\",\r\n\"proxy\":16,\r\n\"proxyIp\":\"127.0.0.1:10809\",\r\n\"proxyUser\":\"xxxx\",\r\n\"proxyPassword\":\"xxx\"\r\n}",
